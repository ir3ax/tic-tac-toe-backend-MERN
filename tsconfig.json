    "compilerOptions": {
        "target": "ES6",
        "module": "commonjs",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "./dist",
        "types": ["node", "dotenv"]
    },
    "include": ["*.ts"],
    "exclude": ["node_modules"]
}
